{
    "manifest_version": 2,
    "name": "Sahibinden Gelişmiş Filtre",
    "version": "1.0",
    "description": "Sahibinden için belirli model arabaları görmemek için ekstra filtre.",
    "author": "Onur Öğüt",
    "author_email": "honorogut@gmail.com",
    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "webNavigation"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      "48": "icon.png"
    }
  }
  